    <function name="Factorial" type="None" variable="">
        <parameters/>
        <body>
            <declare name="num" type="Integer" array="False" size=""/>
            <declare name="i" type="Integer" array="False" size=""/>
            <output expression="&quot;Enter your number: &quot;" newline="True"/>
            <input variable="num"/>
            <for variable="i" start="num-1" end="1" direction="dec" step="1">
                <comment text="for loop can also be increasing, starting from 1 and ending to num - 1. Same procedure applies"/>
                <assign variable="num" expression="num * i"/>
            </for>
            <output expression="&quot;Your number factorial is: &quot; &amp; num" newline="True"/>
        </body>
    </function>
    <function name="IdenticalSums" type="None" variable="">
        <parameters/>
        <body>
            <declare name="num" type="Integer" array="False" size=""/>
            <declare name="arrayLength" type="Integer" array="False" size=""/>
            <declare name="targetSum" type="Integer" array="False" size=""/>
            <output expression="&quot;Enter array length: &quot;" newline="True"/>
            <input variable="arrayLength"/>
            <declare name="array" type="Integer" array="True" size="arrayLength"/>
            <declare name="i" type="Integer" array="False" size=""/>
            <declare name="j" type="Integer" array="False" size=""/>
            <declare name="numPairs" type="Integer" array="False" size=""/>
            <assign variable="numPairs" expression="0"/>
            <declare name="uniqueNumbers" type="Integer" array="False" size=""/>
            <assign variable="uniqueNumbers" expression="0"/>
            <declare name="isUnique" type="Boolean" array="False" size=""/>
            <for variable="i" start="0" end="arrayLength - 1" direction="inc" step="1">
                <output expression="&quot;Enter number in array: &quot;" newline="True"/>
                <input variable="num"/>
                <assign variable="array[i]" expression="num"/>
            </for>
            <comment text="CONNECTOR A"/>
            <declare name="uniqueArray" type="Integer" array="True" size="arrayLength"/>
            <output expression="&quot;Enter target sum: &quot;" newline="True"/>
            <input variable="targetSum"/>
            <for variable="i" start="0" end="arrayLength - 1" direction="inc" step="1">
                <assign variable="isUnique" expression="true"/>
                <if expression="uniqueNumbers = 0">
                    <then>
                        <assign variable="uniqueArray[0]" expression="array[i]"/>
                        <assign variable="uniqueNumbers" expression="uniqueNumbers + 1"/>
                    </then>
                    <else>
                        <for variable="j" start="0" end="uniqueNumbers - 1" direction="inc" step="1">
                            <if expression="array[i] = uniqueArray[j]">
                                <then>
                                    <if expression="array[i] + array[j] = targetSum">
                                        <then>
                                            <comment text="Count for the case when the desired sum is even and there are two numbers equal to the desiredSum/2"/>
                                        </then>
                                        <else>
                                            <assign variable="isUnique" expression="false"/>
                                        </else>
                                    </if>
                                </then>
                                <else/>
                            </if>
                        </for>
                        <if expression="isUnique = true">
                            <then>
                                <assign variable="uniqueArray[uniqueNumbers]" expression="array[i]"/>
                                <assign variable="uniqueNumbers" expression="uniqueNumbers + 1"/>
                            </then>
                            <else/>
                        </if>
                    </else>
                </if>
            </for>
            <comment text="CONNECTOR B"/>
            <for variable="i" start="0" end="uniqueNumbers - 2" direction="inc" step="1">
                <for variable="j" start="i+1" end="uniqueNumbers-1" direction="inc" step="1">
                    <if expression="uniqueArray[i] + uniqueArray[j] = targetSum">
                        <then>
                            <assign variable="numPairs" expression="numPairs + 1"/>
                        </then>
                        <else/>
                    </if>
                </for>
            </for>
            <output expression="&quot;There are &quot; &amp; numPairs &amp; &quot; unique number pairs whose sum is your desired number.&quot;" newline="True"/>
        </body>
    </function>
    <function name="IntersectionOfLists" type="None" variable="">
        <parameters/>
        <body>
            <declare name="array1Length" type="Integer" array="False" size=""/>
            <declare name="array2Length" type="Integer" array="False" size=""/>
            <declare name="i" type="Integer" array="False" size=""/>
            <declare name="j" type="Integer" array="False" size=""/>
            <declare name="num" type="Integer" array="False" size=""/>
            <output expression="&quot;What is the length of the first list? &quot;" newline="True"/>
            <input variable="array1Length"/>
            <output expression="&quot;What is the length of the second list? &quot;" newline="True"/>
            <input variable="array2Length"/>
            <declare name="array1" type="Integer" array="True" size="array1Length"/>
            <declare name="array2" type="Integer" array="True" size="array2Length"/>
            <for variable="i" start="0" end="array1Length-1" direction="inc" step="1">
                <output expression="&quot;Enter a number in the first list: &quot;" newline="True"/>
                <input variable="num"/>
                <assign variable="array1[i]" expression="num"/>
            </for>
            <comment text="CONNECTOR A"/>
            <for variable="i" start="0" end="array2Length-1" direction="inc" step="1">
                <output expression="&quot;Enter a number in the second list: &quot;" newline="True"/>
                <input variable="num"/>
                <assign variable="array2[i]" expression="num"/>
            </for>
            <for variable="i" start="0" end="array1Length -1" direction="inc" step="1">
                <comment text="The intersection algorithm does not count for duplicates. If a number appears twice in any of the arrays, it will be outputed twice."/>
                <for variable="j" start="0" end="array2Length -1" direction="inc" step="1">
                    <if expression="array2[j] = array1[i]">
                        <then>
                            <output expression="array1[i]" newline="True"/>
                        </then>
                        <else/>
                    </if>
                </for>
            </for>
        </body>
    </function>
