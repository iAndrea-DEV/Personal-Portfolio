    <function name="ReverseSentence" type="None" variable="">
        <parameters/>
        <body>
            <declare name="sentence" type="String" array="False" size=""/>
            <declare name="reverseSentence" type="String" array="False" size=""/>
            <assign variable="reverseSentence" expression="&quot;&quot;"/>
            <declare name="i" type="Integer" array="False" size=""/>
            <output expression="&quot;Enter your sentence: &quot;" newline="True"/>
            <input variable="sentence"/>
            <declare name="sentenceCharLength" type="Integer" array="False" size=""/>
            <assign variable="sentenceCharLength" expression="Len(sentence)"/>
            <declare name="charArray" type="String" array="True" size="sentenceCharLength"/>
            <for variable="i" start="0" end="sentenceCharLength -1" direction="inc" step="1">
                <assign variable="charArray[i]" expression="Char(sentence,i)"/>
            </for>
            <for variable="i" start="0" end="sentenceCharLength -1" direction="inc" step="1">
                <assign variable="reverseSentence" expression="reverseSentence &amp; charArray[sentenceCharLength-1-i]"/>
            </for>
            <comment text="Using the Char(String,int) function the full reverse can be done immediately. The array inbetween is used for clarity."/>
            <output expression="reverseSentence" newline="True"/>
        </body>
    </function>
    <function name="SecondSmallest" type="None" variable="">
        <parameters/>
        <body>
            <declare name="listLength" type="Integer" array="False" size=""/>
            <declare name="min1" type="Integer" array="False" size=""/>
            <declare name="min2" type="Integer" array="False" size=""/>
            <declare name="num" type="Integer" array="False" size=""/>
            <declare name="i" type="Integer" array="False" size=""/>
            <input variable="listLength"/>
            <for variable="i" start="0" end="listLength-1" direction="inc" step="1">
                <output expression="&quot;Enter your number: &quot;" newline="True"/>
                <input variable="num"/>
                <if expression="i = 0">
                    <then>
                        <assign variable="min1" expression="num"/>
                    </then>
                    <else>
                        <if expression="i = 1">
                            <then>
                                <assign variable="min2" expression="num"/>
                            </then>
                            <else>
                                <if expression="num &gt; min1 &amp;&amp; num &lt; min2">
                                    <then>
                                        <assign variable="min2" expression="num"/>
                                    </then>
                                    <else>
                                        <if expression="num &lt; min1">
                                            <then>
                                                <assign variable="min2" expression="min1"/>
                                                <assign variable="min1" expression="num"/>
                                            </then>
                                            <else/>
                                        </if>
                                    </else>
                                </if>
                            </else>
                        </if>
                    </else>
                </if>
            </for>
            <output expression="&quot;Second smallest number is: &quot; &amp; min2" newline="True"/>
        </body>
    </function>
    <function name="SortAndFrequency" type="None" variable="">
        <parameters/>
        <body>
            <declare name="arrayLength" type="Integer" array="False" size=""/>
            <declare name="i" type="Integer" array="False" size=""/>
            <declare name="j" type="Integer" array="False" size=""/>
            <declare name="num" type="Integer" array="False" size=""/>
            <output expression="&quot;Enter length of array: &quot;" newline="True"/>
            <input variable="arrayLength"/>
            <declare name="array" type="Integer" array="True" size="arrayLength"/>
            <declare name="numberCache" type="Integer" array="False" size=""/>
            <declare name="frequencyCache" type="Integer" array="False" size=""/>
            <declare name="uniqueNumbers" type="Integer" array="False" size=""/>
            <assign variable="uniqueNumbers" expression="0"/>
            <declare name="uniqueNumbersArray" type="Integer" array="True" size="arrayLength"/>
            <declare name="frequencyArray" type="Integer" array="True" size="arrayLength"/>
            <declare name="uniqueNumber" type="Boolean" array="False" size=""/>
            <for variable="i" start="0" end="arrayLength - 1" direction="inc" step="1">
                <output expression="&quot;Enter number: &quot;" newline="True"/>
                <input variable="num"/>
                <assign variable="array[i]" expression="num"/>
                <assign variable="frequencyArray[i]" expression="0"/>
            </for>
            <comment text="CONNECTOR A"/>
            <for variable="i" start="0" end="arrayLength - 1" direction="inc" step="1">
                <assign variable="uniqueNumber" expression="true"/>
                <if expression="uniqueNumbers = 0">
                    <then>
                        <assign variable="uniqueNumbersArray[0]" expression="array[i]"/>
                        <assign variable="frequencyArray[0]" expression="frequencyArray[0] + 1"/>
                        <assign variable="uniqueNumbers" expression="uniqueNumbers + 1"/>
                    </then>
                    <else>
                        <for variable="j" start="0" end="uniqueNumbers - 1" direction="inc" step="1">
                            <if expression="array[i] = uniqueNumbersArray[j]">
                                <then>
                                    <assign variable="frequencyArray[j]" expression="frequencyArray[j] + 1"/>
                                    <assign variable="uniqueNumber" expression="false"/>
                                </then>
                                <else/>
                            </if>
                        </for>
                        <if expression="uniqueNumber = true">
                            <then>
                                <assign variable="uniqueNumbersArray[uniqueNumbers]" expression="array[i]"/>
                                <assign variable="frequencyArray[uniqueNumbers]" expression="frequencyArray[uniqueNumbers]+ 1"/>
                                <assign variable="uniqueNumbers" expression="uniqueNumbers + 1"/>
                            </then>
                            <else/>
                        </if>
                    </else>
                </if>
            </for>
            <comment text="CONNECTOR B"/>
            <for variable="i" start="0" end="uniqueNumbers-2" direction="inc" step="1">
                <for variable="j" start="i+1" end="uniqueNumbers - 1" direction="inc" step="1">
                    <if expression="frequencyArray[i] &lt; frequencyArray[j]">
                        <then>
                            <assign variable="frequencyCache" expression="frequencyArray[i]"/>
                            <assign variable="frequencyArray[i]" expression="frequencyArray[j]"/>
                            <assign variable="frequencyArray[j]" expression="frequencyCache"/>
                            <assign variable="numberCache" expression="uniqueNumbersArray[i]"/>
                            <assign variable="uniqueNumbersArray[i]" expression="uniqueNumbersArray[j]"/>
                            <assign variable="uniqueNumbersArray[j]" expression="numberCache"/>
                        </then>
                        <else/>
                    </if>
                </for>
            </for>
            <for variable="i" start="0" end="uniqueNumbers - 1" direction="inc" step="1">
                <output expression="&quot;Number &quot; &amp; uniqueNumbersArray[i] &amp; &quot; appears &quot; &amp; frequencyArray[i] &amp; &quot; times.&quot;" newline="True"/>
            </for>
        </body>
    </function>
    <function name="Squares" type="None" variable="">
        <parameters/>
        <body>
            <declare name="num" type="Integer" array="False" size=""/>
            <declare name="arrayLength" type="Integer" array="False" size=""/>
            <declare name="i" type="Integer" array="False" size=""/>
            <declare name="sum" type="Integer" array="False" size=""/>
            <assign variable="sum" expression="0"/>
            <output expression="&quot;How many numbers would you like to add? &quot;" newline="True"/>
            <input variable="arrayLength"/>
            <declare name="array" type="Integer" array="True" size="arrayLength"/>
            <for variable="i" start="0" end="arrayLength-1" direction="inc" step="1">
                <output expression="&quot;Enter your number: &quot;" newline="True"/>
                <input variable="num"/>
                <assign variable="array[i]" expression="num"/>
            </for>
            <for variable="i" start="0" end="arrayLength-1" direction="inc" step="1">
                <assign variable="sum" expression="sum + array[i]*array[i]"/>
            </for>
            <output expression="&quot;The sum of the squares of your numbers is: &quot; &amp; sum" newline="True"/>
        </body>
    </function>
    <function name="Subsequence" type="None" variable="">
        <parameters/>
        <body>
            <declare name="arrayLength" type="Integer" array="False" size=""/>
            <declare name="subLength" type="Integer" array="False" size=""/>
            <declare name="cacheLength" type="Integer" array="False" size=""/>
            <assign variable="cacheLength" expression="0"/>
            <assign variable="subLength" expression="0"/>
            <declare name="i" type="Integer" array="False" size=""/>
            <declare name="j" type="Integer" array="False" size=""/>
            <declare name="k" type="Integer" array="False" size=""/>
            <declare name="num" type="Integer" array="False" size=""/>
            <output expression="&quot;What is the length of your array? &quot;" newline="True"/>
            <input variable="arrayLength"/>
            <declare name="array" type="Integer" array="True" size="arrayLength"/>
            <declare name="subArray" type="Integer" array="True" size="arrayLength"/>
            <declare name="cacheArray" type="Integer" array="True" size="arrayLength"/>
            <declare name="continueSearch" type="Boolean" array="False" size=""/>
            <comment text="CONNECTOR A"/>
            <for variable="i" start="0" end="arrayLength - 1" direction="inc" step="1">
                <comment text="Inputting the Array"/>
                <output expression="&quot;Enter number in your array: &quot;" newline="True"/>
                <input variable="num"/>
                <assign variable="array[i]" expression="num"/>
            </for>
            <for variable="i" start="0" end="arrayLength - 2" direction="inc" step="1">
                <assign variable="continueSearch" expression="true"/>
                <assign variable="cacheArray[0]" expression="array[i]"/>
                <assign variable="cacheLength" expression="1"/>
                <for variable="j" start="i+1" end="arrayLength - 1" direction="inc" step="1">
                    <if expression="continueSearch = true">
                        <then>
                            <if expression="array[j] &gt; array[j-1]">
                                <then>
                                    <assign variable="cacheArray[cacheLength]" expression="array[j]"/>
                                    <assign variable="cacheLength" expression="cacheLength +1"/>
                                </then>
                                <else>
                                    <assign variable="continueSearch" expression="false"/>
                                    <assign variable="cacheArray[cacheLength]" expression="array[j]"/>
                                    <assign variable="cacheLength" expression="cacheLength +1"/>
                                </else>
                            </if>
                        </then>
                        <else/>
                    </if>
                </for>
                <comment text="CONNECTOR B"/>
                <if expression="cacheLength &gt; subLength">
                    <then>
                        <assign variable="subLength" expression="cacheLength"/>
                        <for variable="k" start="0" end="cacheLength-1" direction="inc" step="1">
                            <assign variable="subArray[k]" expression="cacheArray[k]"/>
                            <assign variable="cacheArray[k]" expression="0"/>
                        </for>
                    </then>
                    <else/>
                </if>
                <assign variable="cacheLength" expression="0"/>
            </for>
            <for variable="i" start="0" end="subLength - 1" direction="inc" step="1">
                <output expression="subArray[i]" newline="True"/>
            </for>
        </body>
    </function>
    <function name="SumOfLists" type="None" variable="">
        <parameters/>
        <body>
