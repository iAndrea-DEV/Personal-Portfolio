    <function name="Average" type="None" variable="">
        <parameters/>
        <body>
            <declare name="listLength" type="Integer" array="False" size=""/>
            <declare name="avg" type="Real" array="False" size=""/>
            <declare name="num" type="Integer" array="False" size=""/>
            <declare name="i" type="Integer" array="False" size=""/>
            <assign variable="avg" expression="0"/>
            <output expression="&quot;How many numbers would you like to find the average of? &quot;" newline="True"/>
            <input variable="listLength"/>
            <for variable="i" start="0" end="listLength - 1" direction="inc" step="1">
                <output expression="&quot;Enter number: &quot;" newline="True"/>
