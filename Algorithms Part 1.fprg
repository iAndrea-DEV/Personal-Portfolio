    <function name="ModeOfArray" type="None" variable="">
        <parameters/>
        <body>
            <declare name="num" type="Integer" array="False" size=""/>
            <declare name="arrayLength" type="Integer" array="False" size=""/>
            <declare name="i" type="Integer" array="False" size=""/>
            <declare name="j" type="Integer" array="False" size=""/>
            <declare name="mode" type="Integer" array="False" size=""/>
            <declare name="modeCount" type="Integer" array="False" size=""/>
            <declare name="cache" type="Integer" array="False" size=""/>
            <declare name="cacheCount" type="Integer" array="False" size=""/>
            <assign variable="modeCount" expression="0"/>
            <assign variable="cacheCount" expression="0"/>
            <output expression="&quot;Enter length of array: &quot;" newline="True"/>
            <input variable="arrayLength"/>
            <declare name="array" type="Integer" array="True" size="arrayLength"/>
            <for variable="i" start="0" end="arrayLength - 1" direction="inc" step="1">
                <output expression="&quot;Enter number: &quot;" newline="True"/>
                <input variable="num"/>
                <assign variable="array[i]" expression="num"/>
            </for>
            <comment text="CONNECTOR A"/>
            <for variable="i" start="0" end="arrayLength - 2" direction="inc" step="1">
                <assign variable="cache" expression="array[i]"/>
                <assign variable="cacheCount" expression="1"/>
                <for variable="j" start="i+1" end="arrayLength - 1" direction="inc" step="1">
                    <if expression="array[j] = cache">
                        <then>
                            <assign variable="cacheCount" expression="cacheCount + 1"/>
                        </then>
                        <else/>
                    </if>
                </for>
                <if expression="cacheCount &gt; modeCount">
                    <then>
                        <assign variable="mode" expression="cache"/>
                        <assign variable="modeCount" expression="cacheCount"/>
                    </then>
                    <else/>
                </if>
            </for>
            <output expression="&quot;Mode of array is &quot; &amp; mode &amp; &quot; with a frequency of &quot; &amp; modeCount &amp; &quot; times.&quot;" newline="True"/>
        </body>
    </function>
