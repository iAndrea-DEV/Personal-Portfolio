    <function name="PrimesBelow" type="None" variable="">
        <parameters/>
        <body>
            <declare name="num" type="Integer" array="False" size=""/>
            <declare name="isPrime" type="Boolean" array="False" size=""/>
            <declare name="i" type="Integer" array="False" size=""/>
            <declare name="j" type="Integer" array="False" size=""/>
            <output expression="&quot;What is your number? &quot;" newline="True"/>
            <input variable="num"/>
            <if expression="num = 1">
                <then>
                    <output expression="&quot;There are no primes below and including your number.&quot;" newline="True"/>
                </then>
                <else>
                    <if expression="num = 2">
                        <then>
                            <output expression="&quot;A prime number below and including your number is 2.&quot;" newline="True"/>
                        </then>
                        <else>
                            <for variable="i" start="2" end="num" direction="inc" step="1">
                                <assign variable="isPrime" expression="true"/>
                                <comment text="We are including the number itself in the list if it is a prime. We start from 2 because 1 is by convention not a prime."/>
                                <for variable="j" start="2" end="i-1" direction="inc" step="1">
                                    <if expression="i % j = 0">
                                        <then>
                                            <assign variable="isPrime" expression="false"/>
                                        </then>
                                        <else/>
                                    </if>
                                </for>
                                <if expression="isPrime = true">
                                    <then>
                                        <output expression="&quot;A prime number below and including your number is: &quot; &amp; i" newline="True"/>
                                    </then>
                                    <else/>
                                </if>
                            </for>
                        </else>
                    </if>
                </else>
            </if>
        </body>
    </function>
    <function name="ReadArrayInReverse" type="None" variable="">
        <parameters/>
        <body>
            <declare name="arrayLength" type="Integer" array="False" size=""/>
            <declare name="num" type="Integer" array="False" size=""/>
