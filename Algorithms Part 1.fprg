            <assign variable="avg" expression="avg / listLength"/>
            <output expression="&quot;The average of these numbers is: &quot; &amp; avg" newline="True"/>
        </body>
    </function>
    <function name="CommonElementsNoDuplication" type="None" variable="">
        <parameters/>
        <body>
            <declare name="array1Length" type="Integer" array="False" size=""/>
            <output expression="&quot;Enter length of first array: &quot;" newline="True"/>
            <input variable="array1Length"/>
            <declare name="array2Length" type="Integer" array="False" size=""/>
            <output expression="&quot;Enter length of second array: &quot;" newline="True"/>
            <input variable="array2Length"/>
            <declare name="i" type="Integer" array="False" size=""/>
            <declare name="j" type="Integer" array="False" size=""/>
            <declare name="k" type="Integer" array="False" size=""/>
            <declare name="num" type="Integer" array="False" size=""/>
            <declare name="array1" type="Integer" array="True" size="array1Length"/>
            <declare name="array2" type="Integer" array="True" size="array2Length"/>
            <declare name="commonArray" type="Integer" array="True" size="array1Length"/>
            <declare name="commonLength" type="Integer" array="False" size=""/>
            <assign variable="commonLength" expression="0"/>
            <declare name="isInCommonArray" type="Boolean" array="False" size=""/>
            <for variable="i" start="0" end="array1Length - 1" direction="inc" step="1">
                <output expression="&quot;Enter element in first array: &quot;" newline="True"/>
                <input variable="num"/>
                <assign variable="array1[i]" expression="num"/>
            </for>
            <comment text="CONNECTOR A"/>
            <for variable="i" start="0" end="array2Length - 1" direction="inc" step="1">
                <output expression="&quot;Enter element in second array: &quot;" newline="True"/>
                <input variable="num"/>
                <assign variable="array2[i]" expression="num"/>
            </for>
            <for variable="i" start="0" end="array1Length - 1" direction="inc" step="1">
                <assign variable="isInCommonArray" expression="false"/>
                <for variable="j" start="0" end="array2Length - 1" direction="inc" step="1">
                    <if expression="array1[i] = array2[j]">
                        <then>
                            <for variable="k" start="0" end="commonLength - 1" direction="inc" step="1">
                                <if expression="array2[j] = commonArray[k]">
                                    <then>
                                        <assign variable="isInCommonArray" expression="true"/>
                                    </then>
                                    <else/>
                                </if>
                            </for>
                            <if expression="isInCommonArray = false">
                                <then>
                                    <assign variable="commonArray[commonLength]" expression="array1[i]"/>
                                    <assign variable="commonLength" expression="commonLength +1"/>
                                </then>
                                <else/>
                            </if>
                        </then>
                        <else/>
                    </if>
                </for>
            </for>
            <for variable="i" start="0" end="commonLength -1" direction="inc" step="1">
                <output expression="commonArray[i]" newline="True"/>
            </for>
        </body>
    </function>
    <function name="Duplicates" type="None" variable="">
        <parameters/>
        <body>
            <declare name="num" type="Integer" array="False" size=""/>
            <declare name="arrayLength" type="Integer" array="False" size=""/>
            <declare name="i" type="Integer" array="False" size=""/>
            <declare name="j" type="Integer" array="False" size=""/>
            <declare name="k" type="Integer" array="False" size=""/>
            <output expression="&quot;How many numbers would you like to add?&quot;" newline="True"/>
            <input variable="arrayLength"/>
            <declare name="array" type="Integer" array="True" size="arrayLength"/>
            <declare name="duplicates" type="Integer" array="True" size="arrayLength"/>
            <declare name="numberOfDuplicates" type="Integer" array="False" size=""/>
            <assign variable="numberOfDuplicates" expression="0"/>
            <declare name="isDuplicate" type="Boolean" array="False" size=""/>
            <assign variable="isDuplicate" expression="false"/>
            <for variable="i" start="0" end="arrayLength -1" direction="inc" step="1">
                <output expression="&quot;Enter number: &quot;" newline="True"/>
                <input variable="num"/>
                <assign variable="array[i]" expression="num"/>
            </for>
            <comment text="CONNECTOR A"/>
            <for variable="i" start="0" end="arrayLength-2" direction="inc" step="1">
                <for variable="j" start="i+1" end="arrayLength-1" direction="inc" step="1">
                    <assign variable="isDuplicate" expression="false"/>
                    <if expression="array[j] = array[i]">
                        <then>
                            <if expression="numberOfDuplicates = 0">
                                <then>
                                    <assign variable="duplicates[numberOfDuplicates]" expression="array[j]"/>
                                    <assign variable="numberOfDuplicates" expression="numberOfDuplicates + 1"/>
                                </then>
                                <else>
                                    <for variable="k" start="0" end="numberOfDuplicates-1" direction="inc" step="1">
                                        <if expression="duplicates[k] = array[j]">
                                            <then>
                                                <assign variable="isDuplicate" expression="true"/>
                                            </then>
                                            <else/>
                                        </if>
                                    </for>
                                    <if expression="isDuplicate = false">
                                        <then>
                                            <assign variable="duplicates[numberOfDuplicates]" expression="array[j]"/>
                                            <assign variable="numberOfDuplicates" expression="numberOfDuplicates + 1"/>
                                        </then>
                                        <else/>
                                    </if>
                                </else>
                            </if>
                        </then>
                        <else/>
                    </if>
                </for>
            </for>
            <for variable="i" start="0" end="numberOfDuplicates-1" direction="inc" step="1">
                <output expression="duplicates[i] &amp; &quot; is a duplicate number.&quot;" newline="True"/>
            </for>
        </body>
    </function>
    <function name="EvenOrOdd" type="None" variable="">
        <parameters/>
        <body>
            <declare name="num" type="Integer" array="False" size=""/>
