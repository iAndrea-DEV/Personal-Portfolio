    <function name="NumberOfSortingSteps" type="None" variable="">
        <parameters/>
        <body>
            <declare name="numberOfStepsIncreasing" type="Integer" array="False" size=""/>
            <declare name="numberOfStepsDecreasing" type="Integer" array="False" size=""/>
            <assign variable="numberOfStepsIncreasing" expression="0"/>
            <assign variable="numberOfStepsDecreasing" expression="0"/>
            <declare name="arrayLength" type="Integer" array="False" size=""/>
            <declare name="i" type="Integer" array="False" size=""/>
            <declare name="j" type="Integer" array="False" size=""/>
            <declare name="carry" type="Integer" array="False" size=""/>
            <declare name="num" type="Integer" array="False" size=""/>
            <output expression="&quot;What is the length of your array? &quot;" newline="True"/>
            <input variable="arrayLength"/>
            <declare name="array" type="Integer" array="True" size="arrayLength"/>
            <declare name="arrayDuplicate" type="Integer" array="True" size="arrayLength"/>
            <for variable="i" start="0" end="arrayLength -1" direction="inc" step="1">
                <output expression="&quot;Enter number in array: &quot;" newline="True"/>
                <input variable="num"/>
                <assign variable="array[i]" expression="num"/>
                <assign variable="arrayDuplicate[i]" expression="num"/>
            </for>
            <comment text="CONNECTOR A"/>
            <comment text="Sort Increasing"/>
            <for variable="i" start="0" end="arrayLength-2" direction="inc" step="1">
                <for variable="j" start="i+1" end="arrayLength-1" direction="inc" step="1">
                    <if expression="array[i] &gt; array[j]">
                        <then>
                            <assign variable="carry" expression="array[j]"/>
                            <assign variable="array[j]" expression="array[i]"/>
                            <assign variable="array[i]" expression="carry"/>
                            <assign variable="numberOfStepsIncreasing" expression="numberOfStepsIncreasing + 1"/>
                        </then>
                        <else/>
                    </if>
                </for>
            </for>
            <comment text="Sort Decreasing"/>
            <for variable="i" start="0" end="arrayLength-2" direction="inc" step="1">
                <for variable="j" start="i+1" end="arrayLength-1" direction="inc" step="1">
                    <if expression="arrayDuplicate[i] &lt; arrayDuplicate[j]">
                        <then>
                            <assign variable="carry" expression="array[i]"/>
                            <assign variable="array[i]" expression="array[j]"/>
                            <assign variable="array[j]" expression="carry"/>
                            <assign variable="numberOfStepsDecreasing" expression="numberOfStepsDecreasing + 1"/>
                        </then>
                        <else/>
                    </if>
                </for>
            </for>
            <output expression="&quot;You need &quot; &amp; numberOfStepsIncreasing &amp; &quot; steps to sort this array in an increasing order.&quot;" newline="True"/>
            <output expression="&quot;You need &quot; &amp; numberOfStepsDecreasing &amp; &quot; steps to sort this array in an decreasing order.&quot;" newline="True"/>
        </body>
    </function>
    <function name="OddAndPrime" type="None" variable="">
        <parameters/>
        <body>
            <declare name="num" type="Integer" array="False" size=""/>
            <declare name="arrayLength" type="Integer" array="False" size=""/>
            <declare name="isPrime" type="Boolean" array="False" size=""/>
            <output expression="&quot;Enter length of array: &quot;" newline="True"/>
            <input variable="arrayLength"/>
            <declare name="i" type="Integer" array="False" size=""/>
            <declare name="j" type="Integer" array="False" size=""/>
            <declare name="array" type="Integer" array="True" size="arrayLength"/>
            <declare name="nPrimes" type="Integer" array="False" size=""/>
            <assign variable="nPrimes" expression="0"/>
            <for variable="i" start="0" end="arrayLength - 1" direction="inc" step="1">
                <output expression="&quot;Enter number in array: &quot;" newline="True"/>
                <input variable="num"/>
                <assign variable="array[i]" expression="num"/>
            </for>
            <declare name="oddPrimeArray" type="Integer" array="True" size="arrayLength"/>
            <comment text="CONNECTOR A"/>
            <for variable="i" start="0" end="arrayLength - 1" direction="inc" step="1">
                <assign variable="isPrime" expression="true"/>
                <if expression="array[i] &gt; 2">
                    <then>
                        <for variable="j" start="2" end="array[i] - 1" direction="inc" step="1">
                            <comment text="It can be shown that is suffices to compare the number in array[i] with j from to up to sqrt(array[i]). &#13;&#10;This is because if a number d divides n, then n/d must also divide n. &#13;&#10;Using proof by contradiction, it can be shown that &#13;&#10;one of these numbers must be less than sqrt(n). &#13;&#10;This can reduce the number of comparisons by half."/>
                            <if expression="array[i] % j = 0">
                                <then>
                                    <assign variable="isPrime" expression="false"/>
                                </then>
                                <else/>
                            </if>
                        </for>
                    </then>
                    <else>
                        <comment text="Even though an element can be 2 and it is prime, it is an even prime which we do not want in the array. Therefore boolean is assigned to false"/>
                        <assign variable="isPrime" expression="false"/>
                    </else>
                </if>
                <if expression="isPrime = true">
                    <then>
                        <assign variable="oddPrimeArray[nPrimes]" expression="array[i]"/>
                        <assign variable="nPrimes" expression="nPrimes + 1"/>
                        <comment text="Prime numbers were identified."/>
                    </then>
                    <else/>
                </if>
            </for>
            <comment text="CONNECTOR B"/>
            <declare name="cache" type="Integer" array="False" size=""/>
            <for variable="i" start="0" end="nPrimes - 2" direction="inc" step="1">
                <for variable="j" start="i+1" end="nPrimes - 1" direction="inc" step="1">
                    <if expression="oddPrimeArray[i] &gt; oddPrimeArray[j]">
                        <then>
                            <assign variable="cache" expression="oddPrimeArray[i]"/>
                            <assign variable="oddPrimeArray[i]" expression="oddPrimeArray[j]"/>
                            <assign variable="oddPrimeArray[j]" expression="cache"/>
                        </then>
                        <else/>
                    </if>
                </for>
            </for>
            <for variable="i" start="0" end="nPrimes - 1" direction="inc" step="1">
                <output expression="oddPrimeArray[i]" newline="True"/>
            </for>
        </body>
    </function>
