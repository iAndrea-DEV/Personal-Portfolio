    <function name="SymmetricArray" type="None" variable="">
        <parameters/>
        <body>
            <declare name="arrayLength" type="Integer" array="False" size=""/>
            <declare name="num" type="Integer" array="False" size=""/>
            <declare name="i" type="Integer" array="False" size=""/>
            <declare name="isSymmetric" type="Boolean" array="False" size=""/>
            <assign variable="isSymmetric" expression="true"/>
            <output expression="&quot;What is the length of your array? &quot;" newline="True"/>
            <input variable="arrayLength"/>
            <declare name="array" type="Integer" array="True" size="arrayLength"/>
            <for variable="i" start="0" end="arrayLength - 1" direction="inc" step="1">
                <output expression="&quot;Enter number: &quot;" newline="True"/>
                <input variable="num"/>
                <assign variable="array[i]" expression="num"/>
            </for>
            <if expression="arrayLength % 2 = 0">
                <then>
                    <for variable="i" start="0" end="arrayLength/2 -1" direction="inc" step="1">
                        <if expression="array[i] != array[arrayLength-1-i]">
                            <then>
                                <assign variable="isSymmetric" expression="false"/>
                            </then>
                            <else/>
                        </if>
                    </for>
                </then>
                <else>
                    <for variable="i" start="0" end="(arrayLength -1) /2" direction="inc" step="1">
                        <if expression="array[i] != array[arrayLength-1-i]">
                            <then>
                                <assign variable="isSymmetric" expression="false"/>
                            </then>
                            <else/>
                        </if>
                    </for>
                </else>
            </if>
            <if expression="isSymmetric = true">
                <then>
                    <output expression="&quot;Array is symmetric.&quot;" newline="True"/>
                </then>
                <else>
                    <output expression="&quot;Array is not symmetric.&quot;" newline="True"/>
                </else>
            </if>
        </body>
    </function>
