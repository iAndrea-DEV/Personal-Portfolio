            <declare name="array2" type="Integer" array="True" size="arrayLength"/>
            <declare name="sumArray" type="Integer" array="True" size="arrayLength"/>
            <for variable="i" start="0" end="arrayLength-1" direction="inc" step="1">
                <output expression="&quot;Add number to first list: &quot;" newline="True"/>
                <input variable="num"/>
                <assign variable="array1[i]" expression="num"/>
            </for>
            <for variable="i" start="0" end="arrayLength-1" direction="inc" step="1">
                <output expression="&quot;Add number to second list: &quot;" newline="True"/>
